    </SqlOutput>
    <SqlOutput nullAsNull="false">
       <Sql>($!s0s!$.customnumber2__c/($!s0s!$.customnumber1__c+1))</Sql>
       <Guard>($!s0s!$.customnumber1__c+1)=0</Guard>
