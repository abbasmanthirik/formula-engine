<?xml version="1.0" encoding="UTF-8"?>
<testCase name="testMillisecWithValidDateTimeString">
    <SqlOutput nullAsNull="false">
       <Sql>DATEPART(MILLISECOND,NULL)</Sql>
       <Guard>0=0</Guard>
    </SqlOutput>
    <SqlOutput nullAsNull="true">
       <Sql>DATEPART(MILLISECOND,NULL)</Sql>
       <Guard>0=0</Guard>
    </SqlOutput>
</testCase>
