       <Sql>HOUR(TIME(TIMESTAMP($!s0s!$.dateString__c)))</Sql>
       <Guard>1=0</Guard>
    </SqlOutput>
    <SqlOutput nullAsNull="true">
       <Sql>HOUR(TIME(TIMESTAMP($!s0s!$.dateString__c)))</Sql>
       <Guard>1=0</Guard>
    </SqlOutput>
</testCase>
